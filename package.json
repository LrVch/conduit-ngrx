{
  "name": "conduit-ngrx",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start:app": "ng serve --source-map",
    "start": "node server.js",
    "build": "ng build",
    "build:prod": "ng build --aot --prod --vendor-chunk --extract-css",
    "test": "npm run lint && jest",
    "testOne": "jest --watch",
    "test:watch": "npm run lint && jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf ./dist/",
    "server": "node ./server.js",
    "prod": "npm run clean && npm run build:prod && npm run server",
    "postinstall": "npm run build:prod",
    "analyze": "npm run clean && ng build --aot --prod --vendor-chunk --extract-css --stats-json && webpack-bundle-analyzer ./dist/conduit-ngrx/stats.json",
    "compodoc": "compodoc -p src/tsconfig.app.json",
    "compodoc:serve": "compodoc -p src/tsconfig.app.json -s",
    "format:write": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
    "accessibility:run": "pa11y-ci --threshold 250",
    "accessibility": "npm-run-all --sequential  build:prod --parallel --race server accessibility:run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{ts,json,md}": [
      "prettier --write",
      "tslint -c src/tslint.json",
      "git add"
    ],
    "{src,e2e}/**/*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^7.1.1",
    "@angular/cli": "^7.1.4",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/entity": "^6.1.2",
    "@ngrx/router-store": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.6.0",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "marked": "^0.5.2",
    "moment": "^2.23.0",
    "ngx-material-timepicker": "^2.11.0",
    "normalizr": "^3.3.0",
    "path": "^0.12.7",
    "rxjs": "^6.0.0",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.26",
    "compression": "^1.7.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.7",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.0.3",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@ngrx/schematics": "^6.1.2",
    "@types/jasmine": "^2.8.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.15",
    "angular-testing-library": "0.0.2",
    "codelyzer": "^4.5.0",
    "compression": "^1.7.3",
    "enhanced-resolve": "^3.3.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.4.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.1.0",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.15.3",
    "protractor": "^5.4.1",
    "rimraf": "^2.6.3",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/app/$1",
      "^@env/(.*)$": "<rootDir>/src/environments/$1"
    }
  },
  "engines": {
    "node": "9.8.0",
    "npm": "6.2.0"
  }
}
